        (argument (allow_fall_through <obj> <iobj>)
                  (role at of in <poss> <unknown> like including as across)
                  TARGET_PROP)))

(any-of (shortcut LOOSE_AGAINST_TARGET) (members
        (argument (allow_fall_through <obj> <iobj> to) (role on against) TARGET)
        (argument (allow_fall_through <obj> <iobj> to) (role on against)
                  (any-of (members TARGET_PROP SITE_OF_TARGET)))
        (argument (role <iobj> to)
                  (vprop (predicate launch*)
                      (args (argument (role <obj>) (nprop (predicate ATTACK_N) (block_adj political)
                              (opt_args (any-of (members AGAINST_TARGET ON_TARGET))))))))
        (argument (role <iobj> to)
                  (vprop (predicate launch*)
                      (args (argument (role <obj>) (nprop (predicate ATTACK_N) (block_adj political)
                              (opt_args (argument (role on against) (any-of (members
                                                  TARGET_PROP SITE_OF_TARGET)))))))))))

(all-of (shortcut SOURCE_SAFE) (members SOURCE (mention (block_headword trojan ddos service attack wannacry ddos dos mirai mirai-based leet botnet botnet-based ddos-based dos-based botnets denial-of-service distributed-denial-of-service distributed-dos ddos-for-hire ddos-attack ddos-attacks iot malware-as-a-service tdos pingback ping denial-of-service-attack denial-of-service-attacks dos-for-hire)))) #some cyberattacks that may be mistakenly identified as agents

