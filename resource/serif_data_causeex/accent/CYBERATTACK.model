        (nprop MANY_TO_MANY (predicate QUANTITY GROUP ORDINAL_NUMBER SERIES COUNTING_NUMBER)
               (opt_args SOURCE_REF INCLUDING_SOURCE)
               (propmod (role <ref>) (mprop (predicate responsible)
                                           (args FOR_CYBERATTACK))))
        #hacker uses methods to attack target
        (nprop (predicate METHOD) (propmod (role <obj> <iobj>)
              (vprop (predicate USE POS_VERB) (args (argument (role <sub>) HACKER_SOURCE)))))
        #is a hacker
        (nprop (propmod (role <sub>) (vprop (predicate BE)
                              (args (argument (role <obj> <iobj>) HACKER_SOURCE)))))))

(any-of (shortcut HACKER_SUB) (members
        (argument (role <sub> by) (allow_fall_through) HACKER_NPROP)))
(any-of (shortcut HACKER_OBJ) (members
        (argument (role to <obj> <iobj>) (allow_fall_through) HACKER_NPROP)))
(argument (shortcut HACKER_REF) (role <ref>) HACKER_NPROP)
(argument (shortcut FROM_HACKER) (role from) HACKER_NPROP)

(any-of (shortcut CRIMINAL_SOURCE) (members HACKER_NPROP
        (nprop (predicate CRIMINAL) (opt_args SOURCE_REF IN_SOURCE LOOSE_OF_SOURCE))
        (nprop (args (argument (role <ref>) (mention (acetype PER ORG FAC))) SOURCE_REF) (adj MALICIOUS))
        (nprop (predicate actor) (adj unknown)) #beware vague
        (nprop (predicate artist artists actor actors guy guys)
               (adj threat bad MALICIOUS CRIMINAL HACKER DDOS PHISH)
               (block_adj news)
               (opt_args SOURCE_REF IN_SOURCE LOOSE_OF_SOURCE))))

(anyprop (shortcut IMPERSONATOR) (opt_args SOURCE_REF)
         (propmod (role <sub>) (any-of (members
                (vprop (predicate impersonat*))
                (vprop (predicate represent*) (args
                        (argument (role <obj> <iobj>) (mention (headword themselves himself herself)))))
                (vprop (predicate pretend*) (args (argument (role <obj> <iobj>) (vprop (predicate BE POS_VERB)))))
                (vprop (predicate pos*) (args (argument (role as) (mention (acetype PER)))))
                (vprop (predicate pos*) (particle as))))))

(any-of (shortcut CRIMINAL_NPROP) (members CRIMINAL_SOURCE IMPERSONATOR
        (nprop (predicate QUANTITY GROUP ORDINAL_NUMBER SERIES)
               (opt_args SOURCE_REF IN_SOURCE LOOSE_OF_SOURCE INCLUDING_SOURCE)
               (args (argument (role of including) CRIMINAL_SOURCE)))
        (nprop (predicate QUANTITY GROUP ORDINAL_NUMBER SERIES)
               (opt_args SOURCE_REF IN_SOURCE LOOSE_OF_SOURCE INCLUDING_SOURCE)
               (propmod (role <ref>) (mprop (predicate responsible)
                      (args (argument (role for) (any-of (members CYBERATTACK
                                                      (nprop (predicate ATTACK_N CAMPAIGN)))))))))
        (nprop (predicate METHOD) (propmod (role <obj> <iobj>)
              (vprop (predicate POS_VERB USE) (args (argument (role <sub>) CRIMINAL_SOURCE)))))))

(any-of (shortcut CRIMINAL_SUB) (members (argument (role <sub> by) CRIMINAL_NPROP)))
(any-of (shortcut CRIMINAL_OBJ) (members (argument (role <obj> <iobj> that to) CRIMINAL_NPROP)))
(any-of (shortcut TO_CRIMINAL) (members (argument (role to) CRIMINAL_NPROP)))

(any-of (shortcut INTERNET_USER) (members (nprop (predicate user*) (adj ONLINE) (opt_args TARGET_PER_REF))
        (nprop (predicate user*) (args EMAIL_REGEX) (opt_args TARGET_REF_EXP))
        (nprop (adj ONLINE) (args TARGET_PER_REF))
        (nprop (args EMAIL_REGEX TARGET_PER_REF))))

<!-- Emails and other messages -->
(any-of (shortcut EMAIL_NPROP) (members (anyprop (args EMAIL_REGEX))
        (nprop (predicate EMAIL_N) (opt_args FROM_SOURCE TO_TARGET))
        (nprop (predicate MESSAGE) (adj text) (opt_args FROM_SOURCE TO_TARGET))
        (vprop (args (argument (role <sub>) (mention (headword EMAIL_N))))
                                (opt_args FROM_SOURCE TO_TARGET TARGET_OBJ_EXP))
        (vprop (args (argument (role <sub>) (mention (headword EMAIL_N)))
            (argument (role <obj>) (anyprop (opt_args FROM_SOURCE TARGET_REF_EXP)
                          (propmod (role <sub>) (nprop (predicate REPLY FOLLOW_LINK)
                                                       (opt_args FROM_SOURCE)))))))))

(any-of (shortcut MESSAGE_NPROP) (members EMAIL_NPROP
        (nprop (predicate MESSAGE LINK) (opt_args FROM_SOURCE TO_TARGET))
        (vprop (args (argument (role <sub>) (mention (headword MESSAGE LINK))))
                                (opt_args FROM_SOURCE TO_TARGET TARGET_OBJ_EXP))
        (vprop (args (argument (role <sub>) (mention (headword MESSAGE LINK)))
            (argument (role <obj>) (anyprop (opt_args FROM_SOURCE TARGET_REF_EXP)
                          (propmod (role <sub>) (nprop (predicate REPLY FOLLOW_LINK)
                                                       (opt_args FROM_SOURCE)))))))))

(argument (shortcut MESSAGE_SUB) (role <sub> by) MESSAGE_NPROP)
(argument (shortcut MESSAGE_OBJ) (role <obj> <iobj> that to) MESSAGE_NPROP)
(argument (shortcut WITH_MESSAGE) (role by via with over) MESSAGE_NPROP)
(argument (shortcut EMAIL_SUB) (role <sub> by) EMAIL_NPROP)
(argument (shortcut EMAIL_OBJ) (role <obj> <iobj> that to) EMAIL_NPROP)
(argument (shortcut WITH_EMAIL) (role by via with over) EMAIL_NPROP)
)
