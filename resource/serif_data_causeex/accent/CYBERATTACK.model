(any-of (shortcut TO_SOURCE_EXP) (members (argument (role to into) SOURCE_SAFE)
        (argument (role to into) SOURCE_PROP)))

(any-of (shortcut FROM_SOURCE_EXP) (members
        (argument (role from by) SOURCE_SAFE)
        (argument (role from by) SOURCE_PROP)))

(any-of (shortcut LOOSE_FROM_SOURCE) (members
        (argument (role <unknown> <poss> from by) SOURCE_SAFE)
