      #has a history of
      (nprop (predicate history) (propmod (role <obj>)
                  (vprop (predicate have has had having)
                          (args (argument (role <sub> by) SOURCE_SAFE)))))
      #actor's methods to...
      (nprop (predicate METHOD)
          (propmod (role <obj> <iobj>) (vprop (predicate POS_VERB USE)
                      (args (argument (role <sub> by) SOURCE_SAFE)))))
      #get instructions to
      (nprop (predicate instructions) (propmod (role <obj> <iobj>) (vprop (predicate GET)
                                    (args (argument (role <sub> by) SOURCE_SAFE)))))
      #accused of
      (anyprop (propmod (role of) (vprop (predicate accused)
                      (args (argument (role <obj> <iobj>) SOURCE_SAFE)))))))

(any-of (shortcut SOURCE_SUB_EXP) (members (argument (role <sub> by) SOURCE_SAFE)
