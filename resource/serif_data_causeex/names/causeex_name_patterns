(toplevel

(regex (return (type ORG) (force YES))
       (re (text (string IMPORTANT_ORG_NAMES))))

(regex (return (type PER) (force YES))
       (re (text (string IMPORTANT_PER_NAMES))))

(regex (return (type GPE) (force YES))
       (re (text (string IMPORTANT_GPE_NAMES))))

(regex (return (type LOC) (force YES))
       (re (text (string IMPORTANT_LOC_NAMES))))

(regex (return (type FAC) (force YES))
       (re (text (string IMPORTANT_FAC_NAMES))))

(regex (return (type WEA) (force YES))
       (re (text (string IMPORTANT_WEA_NAMES))))

(regex (return (type NONE) (force YES))
       (re (text (string IMPORTANT_NONE_NAMES))))


<!-- Tag Chad as GPE, but only when not preceeded by lake
 and not followed by basin. These words imply there's a
 different name type here -->
(regex (return (type GPE) (force YES))
       (re (text (string "(?<!lake )chad(?! basin| gandiya)"))))

<!-- Lake Chad is LOC, except when followed by Basin -->
(regex (return (type LOC) (force YES))
       (re (text (string "lake chad(?! basin)"))))

<!-- Lake Chad Basin is LOC, except when followed by Commission -->
(regex (return (type LOC) (force YES))
       (re (text (string "lake chad basin(?! commission)"))))

<!-- Some cities of Kaliningrad area have some exceptions -->
(regex (return (type GPE) (force YES))
       (re (text (string "(?<!strait of )baltiysk(?! naval)"))))
(regex (return (type GPE) (force YES))
       (re (text (string "chernyakhovska(?! street)"))))
(regex (return (type GPE) (force YES))
       (re (text (string "(?<!vladimir )gusev(?! district)"))))
(regex (return (type GPE) (force YES))
       (re (text (string "neman(?! river)"))))
(regex (return (type GPE) (force YES))
       (re (text (string "primorye(?! maritime)"))))
(regex (return (type ART) (force NO))
       (re (text (string "(natural gas)|(gas)|(oil)"))))


)
)
